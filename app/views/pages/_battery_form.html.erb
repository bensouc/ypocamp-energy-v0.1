<div class="form-box d-none " id='battery' data-enroll-target='formBox'>
  <h2><i class="fa-solid fa-car-battery"></i> BATTERIE CELLULE 3/5</h2>
  <h1>Quel type de batterie avez-vous ?</h1>
  <%= form.input :battery, required: true,label: false, as: :radio_buttons,
                                    collection: ["Plomb", "AGM","Gel","Lithium"],
                                    collection_wrapper_tag: 'div',
                                    collection_wrapper_class: 'wrapper',
                                    item_wrapper_class: 'domain-item',
                                    html5: true,
                                    input_html: { class: 'selector domain-selector',
                                    data: {calculate_energy_target: 'battery',
                                          action:'change->calculate-energy#changeInForm change->enroll#displayNext'}
                                    }
                                    %>
  <%# <div class="nav-btn">
    <div class='previous-btn mt-3' data-action='click->enroll#displayBack'></div>
    <div class='next-btn mt-3' data-action='click->enroll#displayNext'></div>
  </div> %>
  <div class="bottom-navbar mt-3" >
    <div class="logo-navabar inactive" data-action='click->enroll#goToFridge'>
      <i class="fa-solid fa-snowflake"></i>
    </div>
    <div class="logo-navabar inactive" data-action='click->enroll#goToSolar'>
      <i class="fa-solid fa-solar-panel"></i>
    </div>
    <div class="logo-navabar active" data-action='click->enroll#goToBattery'>
      <i class="fa-solid fa-car-battery"></i>
    </div>
    <div class="logo-navabar inactive" data-action='click->enroll#goToKm'>
      <i class="fa-solid fa-gauge-high"></i>
    </div>
    <div class="logo-navabar inactive"data-action='click->enroll#goToFeatures'>
      <i class="fa-solid fa-list-check"></i>
    </div>
  </div>
</div>
